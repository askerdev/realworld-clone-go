meta {
  name: Articles Favorited by Username with auth
  type: http
  seq: 12
}

get {
  url: {{APIURL}}/articles?favorited={{USERNAME}}
  body: json
  auth: none
}

params:query {
  favorited: {{USERNAME}}
}

headers {
  Content-Type: application/json
  X-Requested-With: XMLHttpRequest
  Authorization: Token {{token}}
}
